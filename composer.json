{
    "name"        : "En Mapper",
    "description" : "An automatic Mapper for Encounter Locations",
    "authors"     : [
        {
            "name"  : "David Tabachnikov",
            "email" : "nethunter@studiosh.com"
        }
    ],
    "require": {
        "php"                   : ">=5.3.3",
        "silex/silex"           : "dev-master",
        "twig/twig"             : "1.*",
        "monolog/monolog"       : "1.0.*",
        "symfony/form"          : "~2.2",
        "symfony/translation"   : "~2.2",
        "symfony/twig-bridge"   : "~2.2",
        "symfony/validator"     : "~2.2",
        "symfony/yaml"          : "~2.2",
        "symfony/config"        : "~2.2",
        "symfony/console"       : "~2.2",
        "kriswallsmith/assetic" : "1.0.*",
        "twitter/bootstrap"     : "v2.2.2",
        "mheap/silex-assetic"   : "dev-master",
        "php-google-maps/php-google-maps": "*",
        "fabpot/goutte"         : "1.0.*",
        "mobiledetect/mobiledetectlib": "2.5.*",
        "symfony/dom-crawler"   : "~2.2",
        "symfony/css-selector"  : "~2.2",
        "symfony/browser-kit"   : "~2.2",
        "doctrine/dbal"         : "2.3.*",
        "doctrine/orm"          : "2.3.*",
        "taluu/doctrine-orm-provider" : "1.0.*",
        "treffynnon/navigator": "dev-master",
        "symfony/security": "~2.2",
        "dreamfactory/charisma": "dev-master",
        "silex/web-profiler": "dev-master"
    },
    "require-dev": {
        "phing/phing"           : "2.4.*",
        "silex/web-profiler": "dev-master"
    },
    "minimum-stability" : "dev",
    "scripts": {
        "post-install-cmd": "SKE\\Composer\\Script::postInstall"
    },
    "autoload": {
        "psr-0": {
            "SKE": "src/",
            "En": "src/"
        }
    }
}
